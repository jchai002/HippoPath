<!DOCTYPE html>
<html>
<head>
  <title>HippoPath</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>


<%= render "layouts/header" %>
<div class="container">
<%if notice && !@no_flash%>
<div class="alert alert-success" role="alert"><%= notice %></div>
<%end%>

<%= yield %>
</div>

</body>
</html>
